pipeline {
    agent any
    stages {
        stage('Hello') {
            steps {
                sh 'env'
                sh 'id'
                sh 'whoami'
                sh 'ls -a'
                sh 'ls -la /'
                sh 'ls -la /var/'
                sh 'whoami'
                sh 'id'
            }
        }
    }
}
